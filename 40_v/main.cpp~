#include <iostream>
#include <string>
using namespace std;

int main () {
  
  // string is an array of characters 
  string name = "Mohammed Abu-Hadhoud";
  cout << name[0] << endl;
  cout << name[2] << endl;
  cout << name[7] << endl;

  // example of an array of integers 
  int x[5] = {22, 18, 2, 55, 520};

  cout << x[0] << endl;
  cout << x[1] << endl;
  cout << x[2] << endl;
  cout << x[3] << endl;
  cout << x[4] << endl;

  return 0;
}
