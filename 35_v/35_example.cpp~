#include <iostream>
#include <string>
using namespace std;

int mySumFunction(int num1, int num2) {

  return num1 + num2;
}

int main() {
  int num1, num2;
  cout << "Enter number 1: "; 
  cin >> num1;
  cout << "Enter number 2: "; 
  cin >> num2;

  cout << mySumFunction(num1, num2) << endl;


}
